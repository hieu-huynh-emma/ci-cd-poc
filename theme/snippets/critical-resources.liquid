<script src="//unpkg.com/@ungap/custom-elements"></script>
<script src="{{ 'quickscript-beta.min.js' | asset_url }}"></script>


<script defer src="{{ 'delay-resource.js' | asset_url }}"></script>
<script is="delay-script" url="{{ 'first-interactive.min.js' | file_url }}"></script>

{% if isOptimized == true %}
  <script defer src="{{ 'jquery-3.6.0.min.js' | file_url }}"></script>

  <script defer>
      const Shop = {
          id: "{{ shop.id | json }}",
          assetUrl: "{{ "a" | asset_url }}".match(/^.*\//g)[0],
          fileUrl: "{{ "a" | file_url }}".match(/^.*\//g)[0],
          fileImgUrl: "{{ "a" | file_img_url }}".match(/^.*\//g)[0],
          settings: {
              saleTimer: "{{ settings.sale_timer }}"
          },
          addonServiceIds: "{{ shop.metafields.globals.addon_services }}"
      }
  </script>

  {% render 'critical-stylesheet' %}
{% else %}
  <script src="{{ 'jquery-3.6.0.min.js' | file_url }}"></script>
  <script src="{{ 'helpers.js' | asset_url }}"></script>

  <script>
      const Shop = {
          id: "{{ shop.id | json }}",
          assetUrl: "{{ "a" | asset_url }}".match(/^.*\//g)[0],
          fileUrl: "{{ "a" | file_url }}".match(/^.*\//g)[0],
          fileImgUrl: "{{ "a" | file_img_url }}".match(/^.*\//g)[0],
          settings: {
              saleTimer: "{{ settings.sale_timer }}"
          },
          addonServiceIds: "{{ shop.metafields.globals.addon_services }}"
      }
  </script>
  {% render 'critical-stylesheet' %}
{% endif %}


