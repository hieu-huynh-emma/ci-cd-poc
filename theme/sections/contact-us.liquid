                <div>
                    <h3 class="text-md">Email us</h3>
                    <p class="text-comet">
                        Drop us an email at <a class="underline cursor-pointer"
                                      href="mailto:{{ shop.email }}">{{ shop.email }}</a>
                        and we'll respond in 24-48 hours.</p>
                </div>
            </div>
        </div>

        <div class="section-ribbon">
            <img class="lazyload cover-surface" data-src="{{ 'Smile.png' | file_img_url: '996x506' }}" width="996"
                 height="506" alt="">
        </div>
    </div>

    <script type="quickscript" loading="visibility" src="{{ 'contact-us.js' | asset_url }}"></script>
