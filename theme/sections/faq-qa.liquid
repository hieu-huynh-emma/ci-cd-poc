<div id="{{ section.settings.slug }}" class="qna-section py-4 lg:py-24">
  <div class="mx-auto max-w-7xl px-4 sm:px-3 md:px-5">
    <div class="lg:flex lg:gap-60">
      <h3 class="font-inter mb-5 text-center text-[26px] lg:mb-0 lg:w-1/4 lg:text-left lg:text-[39px]">{{ section.settings.title }}</h3>
      <div class="lg:flex-1">
        {% for block in section.blocks %}
          <article data-search="{{ block.settings.question }}" data-search-term="{{ block.settings.question | strip | downcase }}" class="accordion-wrapper py-2">
            <div class="accordion flex items-center justify-between py-2 w-full cursor-pointer">
              <span class="flex-1 font-inter text-sm font-semibold lg:text-xl">{{ block.settings.question }}</span>
              <svg class="accordion-icon transition-transform" width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <rect x="0.000610352" width="20" height="19.5996" fill="url(#pattern1)" />
                <defs>
                  <pattern id="pattern1" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_8_6065" transform="scale(0.001001 0.00102145)" />
                  </pattern>
                  <image
                    id="image0_8_6065"
                    width="999"
                    height="979"
                    xlink:href="data:image/png;base64,"
                  />
                </defs>
              </svg>
            </div>
            <div class="accordion__content font-inter">
              <p class="faq-answer text-[11px] lg:text-base">{{ block.settings.answer | remove: "<p>" | remove: "</p>" | strip }}</p>
              {% if block.settings.html != blank %}
                <div class="mt-3 text-[11px] lg:text-base">{{ block.settings.html }}</div>
              {% endif %}
            </div>
          </article>
        {% endfor %}
      </div>
    </div>
  </div>
</div>

<script type="module">
  $(document).ready(function () {
    $('.accordion-wrapper').beefup({
      trigger: ".accordion",
      content: ".accordion__content",
      openSpeed: 100,
      closeSpeed: 100
    });
  })
</script>

{% schema %}
{
  "name": "Question & Answer",
  "class": "faq-qa-section",
  "tag": "section",
  "settings": [
    {
      "id": "title",
      "type": "text",
      "label": "Title"
    },
    {
      "id": "slug",
      "type": "text",
      "label": "Section slug",
      "info": "Question & Answer section slug should be unique and follow slug format. Slug format example: top-questions"
    }
  ],
  "blocks": [
    {
      "type": "qna",
      "name": "QnA",
      "settings": [
        {
          "id": "question",
          "type": "text",
          "label": "Question"
        },
        {
          "id": "answer",
          "type": "richtext",
          "label": "Answer"
        },
        {
          "type": "html",
          "id": "html",
          "label": "HTML embed"
        }
      ]
    }
  ]
}
{% endschema %}
