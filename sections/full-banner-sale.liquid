{% schema %}
  {
    "name": "Full Banner Sale CTA",
    "settings": [
					{
						"type": "text",
						"id": "cta-top-sub",
						"label": "CTA Top Subtitle"
					},
					{
						"type": "text",
						"id": "cta-title",
						"label": "CTA Title"
					},
					{
						"type": "text",
						"id": "cta-button-text",
						"label": "CTA Button Text"
					},
					{
                        "type": "url",
                        "id": "cta-button-link",
                        "label": "CTA Button Link"
                    },
					{
                        "type": "image_picker",
                        "id": "cta-bg-img",
                        "label": "CTA Background Image"
                    }
				],
	"blocks": 	[
				],
	"presets":	[
					{
						"category": "Advance Layout",
						"name": "Custom Sale Banner CTA"
					}
				]
  }
{% endschema %}

{% stylesheet %}

	@media (max-width: 849px) {
        .desktop {
          display: none;
        }
      }

      @media (min-width: 850px) {
        .desktop {
          display: block;
        }
      }


		@media (max-width: 849px) {
        .mobile {
          display: block;
        }
      }

      @media (min-width: 850px) {
        .mobile {
          display: none;
        }
      }





{% endstylesheet %}

