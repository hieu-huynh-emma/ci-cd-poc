    "featured-faqs": {
      "type": "featured-faqs",
      "blocks": {
        "78ca144d-5919-4a1b-9e22-082ef5384310": {
          "type": "faq",
          "settings": {
            "title": "Is it free to return your products?",
            "content": "<p>It's 100% free to return your Emma! If your Emma turns out not to suit you for any reason, feel free to return it. Just simply get in contact with us via our chat support, phone or email and we will refund and collect it at no cost to you.Live support is also available from 08:00 to 18:00 GMT-6, Monday-Friday!<\/p>"
          }
        },
        "c68412f4-0251-42dc-9113-a0eecd22c518": {
          "type": "faq",
