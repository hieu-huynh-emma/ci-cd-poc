{
  "name": "shopify-template-ca",
  "version": "1.0.0",
  "description": "Shopify template for Canada",
  "scripts": {
    "start": "concurrently npm:serve:* --raw",
    "serve:theme": "npm run shopify:switch && shopify theme serve",
    "serve:scss": "gulp watch",
    "gulp:sass": "gulp sass",
    "gulp:sass-uncompressed": "gulp sass-uncompressed",
    "gulp:clean": "gulp clean",
    "pull:live": "npm run shopify:switch && shopify theme pull -t CA-MAIN",
    "push:dev": "npm run shopify:switch && shopify theme push -t CA-DEV",
    "shopify:login": "shopify login --store=emma-sleep-ca",
    "shopify:switch": "shopify switch --store=emma-sleep-ca",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emma-sleep/shopify-template-ca.git"
  },
  "keywords": [
    "shopify"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emma-sleep/shopify-template-ca/issues"
  },
  "homepage": "https://github.com/emma-sleep/shopify-template-ca#readme",
  "dependencies": {
    "cssnano": "^5.1.13",
    "cssnano-preset-advanced": "^5.3.8",
    "concurrently": "^7.3.0",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-cached-sass": "^0.0.5",
    "gulp-flatten": "^0.4.0",
    "gulp-sass": "^5.1.0",
    "gulp-postcss": "^9.0.1",
    "gulp-purgecss": "^4.1.3",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.16"
  }
}
